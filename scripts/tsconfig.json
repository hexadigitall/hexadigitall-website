{
  "compilerOptions": {
    "target": "ES2019",
  "module": "es2020",
    "moduleResolution": "bundler",
    "ignoreDeprecations": "6.0",
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["node", "jest"],
  "noEmit": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  "strict": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.test.ts",
    "./**/*.test.tsx"
  ]
}